<%- include('header',{'type':'post','pname':'日志'});%>
		<!-- begin content -->
		<div id="content-wrap" class="post">
			<div class="row wide">
				<div class="col-eight tab-full">
					<div class="post-container message-card">
						<%const placeholder='holder.js/10x10?auto=yes&random=yes&text=Mira.Sofia~'%>
						<img src="<%=describe.cover%>" data-src="<%=(describe.cover)&&describe.cover.length>0?'':placeholder%>" draggable="false">
						<div class="overlay"></div>
						<div class="post-content">
							<h1 class="post-title">
								<a href="#" title=""><%=describe.name%></a>
							</h1>
							<ul class="post-date">
								<li><%=describe.create_time%></li>
								<li>
									<span><%=describe.category%></span>
								</li>
								<%if(config.nav&&config.nav.busuanzi&&config.nav.busuanzi==true){%>
								<script async src="//busuanzi.ibruce.info/busuanzi/2.3/busuanzi.pure.mini.js"></script>
								<li>
									<%var family=config.iconfont&&config.iconfont.family?config.iconfont.family:'engineer'%>
									<span class="<%=family%> icon-view" style="margin-left:5px;"></span>
									<span id="busuanzi_value_page_pv">0</span>
								</li>
								<%}%>
							</ul>
						</div>
					</div>
					<article class="post-body message-card">
						{{content}}
					</article>
					<%if(config.comment&&config.comment.gitalk&&config.comment.gitalk.clientID){%>
						<%- include('comment',{type: "post",'pname':'文章'});%>
					<%}%>
				</div>
				<div class="col-four tab-full">
					<div class="infinity-widget widget_categories">
						<div class="widget-title">
							<h3>TAGS</h3>
						</div>
						<p class="category">
						<%if(describe.tags&&describe.tags.length>0){%>
							<%describe.tags.forEach(function(tag){%>
								<a href="#"><%=tag%></a>
							<%});%>
						<%} else {%>
							<a href="#">无</a>
						<%}%>
						</p>
					</div>
					<div>
						<%const advImage=config.adv?config.adv.image:null%>	
						<a href="<%=config.adv?config.adv.link:'#'%>" target="<%=config.adv?config.adv.target:'_self'%>">
							<img src="<%=advImage%>" data-src="<%=advImage?advImage:placeholder%>" style="width: 100%;max-height:140px;object-fit:cover;" draggable="false" alt="<%=config.user.name%>"/>
						</a>
					</div>
				</div>
			</div>
		</div>
		<!-- end content -->
<%- include('footer');%>
